[flake8]
max-line-length = 88
count = true
statistics = true
show_source = false
select = E,F,W
extend-exclude = *_pb2.py
extend-ignore =
    # module level import not at top of file
    E402,
    # line too long
    E501,
    # '<module>' imported but unused
    F401,
    # line break before binary operator
    W503,
